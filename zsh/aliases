# cd
alias ..='cd ..'

# ls
alias ls="ls -G"
alias l="ls -lAGh"
alias ll="ls -la"
alias la='ls -A'

# rails
alias scaffold='script/generate nifty_scaffold'
alias migrate='rake db:migrate db:test:clone'

# commands starting with % for pasting from web
alias %=

# editor
alias mate='/Applications/Fraise.app/Contents/MacOS/Fraise'
